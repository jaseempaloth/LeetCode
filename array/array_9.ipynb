    "1636. Sort Array by Increasing Frequency"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "\n",
    "from collections import Counter\n",
    "def frequencySort(self, nums: list[int]) -> list[int]:\n",
    "    count = Counter(nums)\n",
    "    nums.sort(key=lambda x: (count[x], -x))\n",
    "    return nums\n",
    "\n",
    "\n"
   ]
  }
